<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="Eighteentech_CustomForm::config_eighteentech_customform" title="Custom Form Section" sortOrder="50">
                                <resource id="Eighteentech_CustomForm::save" title="Save Record" sortOrder="10" />
                                <resource id="Eighteentech_CustomForm::delete" title="Delete Record" sortOrder="20" />
                            </resource>
                        </resource>
                    </resource>
                </resource>
                <resource id="Eighteentech_CustomForm::customform_manage" title="Custom Form" translate="title" sortOrder="10">
					<resource id="Eighteentech_CustomForm::customform_manage_items" title="Manage Custom Form" translate="title" sortOrder="10">
						<resource id="Eighteentech_CustomForm::customform" title="Custom Form" translate="title" sortOrder="10" />
						<resource id="Eighteentech_CustomForm::customform_save" title="Custom Form" translate="title" sortOrder="10" />
					</resource>	
					<resource id="Eighteentech_CustomForm::customform_configuration" title="Configuration" translate="title" sortOrder="10" />
                </resource>
            </resource>
        </resources>
    </acl>
</config>
